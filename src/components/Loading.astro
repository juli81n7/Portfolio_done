<section>
  <div class="container">
    <img class="logo" src="../../svg/logo.svg" alt="" />
    <img class="text" src="svg/logo_round.svg" alt="" />
  </div>
  <h1>
    100
  </h1>
</section>

<style>
  .logo {
    scale: 0.5;
    grid-column: 1;
    grid-row: 1;
    animation-name: rotate;
    animation-duration: 7s;
    animation-iteration-count: infinite;
    animation-timing-function: linear;
  }
  .text {
    scale: 1;
    grid-column: 1;
    grid-row: 1;
    animation-name: rotate_back;
    animation-duration: 7s;
    animation-iteration-count: infinite;
    animation-timing-function: linear;
    transform: rotate(-360deg);
  }
  section {
    background-color: var(--primary);
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 999999;
    position: absolute;
  }

  h1{
    position: absolute;
    right: 1rem;
    bottom: 0rem;
    font-weight: 600 !important;
    font-size: 4rem!important;
    text-align: right;
  }

  .container {
    display: grid;
    grid-template-columns: 1fr;
    grid-template-rows: 1fr;
  }

  @keyframes rotate {
    to {
      transform: rotate(-360deg);
    }
}
    @keyframes rotate_back {
      to {
        transform: rotate(0deg);
      }
    }
  }

</style>

