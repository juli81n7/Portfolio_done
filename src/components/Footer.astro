<style>
  footer {
    background-color: var(--secondary);
    color: var(--primary);
    overflow: hidden;
    padding-bottom: 3rem;
    padding-left: 3rem;
    padding-right: 3rem;
  }

  footer h3 {
    margin: 0;
    margin-top: 1rem;
    color: var(--primary);
  }

  footer p {
    margin: 0;
    color: var(--primary);
  }
  .link_footer {
    opacity: 0%;
    transition: 0.3s ease-in-out;
  }

  .link_footer svg polygon {
    fill: var(--primary);
  }
  .link_footer svg line {
    stroke: var(--primary);
  }

  .footer_brand svg {
    fill: var(--primary);
    width: 5rem;
    margin-left: auto;
    margin-right: auto;
    transition: 0.5s ease;
  }

  .footer_rotate:hover {
    transform: rotate(45deg);
    transition: 0.5s ease;
  }

  footer {
    display: grid;
    grid-template-columns: 1fr 6fr;
    gap: 2rem;
  }

  footer .grid_1_1_1 {
    width: 100%;
    display: flex;
    justify-content: space-evenly;
    margin-top: auto;
    margin-bottom: auto;
  }
</style>
